
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>TablaMultiplicar</title>
</head>
<body>
    <div>
        <table border="1">
            @for (int i = 1; i <= 12; i++)
            {
                <tr>
                    @for (int j = 1; j <= 10; j++)
                    {
                        <td>@i * @j = @(i * j)</td>
                    }
                </tr>
            }
        </table>
    </div>
</body>
</html>
